{
  "name": "@intuvance/esm-llm-tools-app",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "description": "LLM tools app for frontend medical practitioners to use A.I ",
  "browser": "dist/intuvance-esm-llmtools-app.js",
  "main": "src/index.ts",
  "source": true,
  "scripts": {
    "start": "openmrs develop",
    "serve": "webpack serve --mode=development",
    "debug": "npm run serve",
    "test": "cross-env TZ=UTC vitest run --passWithNoTests",
    "test:watch": "cross-env TZ=UTC vitest watch",
    "build": "webpack --mode=production",
    "build:development": "webpack --mode=development",
    "analyze": "webpack --mode=production --env analyze=true",
    "typescript": "tsc",
    "lint": "eslint src --ext ts,tsx"
  },
  "keywords": [
    "openmrs",
    "microfrontends"
  ],
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intuvance/esm-expertsystem.git"
  },
  "bugs": {
    "url": "https://github.com/intuvance/esm-expertsystem/issues"
  },
  "homepage": "https://github.com/intuvance/esm-expertsystem#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@carbon/react": "1.x",
    "@openmrs/esm-framework": "*",
    "react": "18.x",
    "react-dom": "18.x",
    "react-i18next": "11.x",
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@openmrs/esm-framework": "^7.0.1-pre.3291",
    "@openmrs/webpack-config": "^6.3.0",
    "cross-env": "^7.0.3",
    "happy-dom": "^17.4.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.18.6",
    "vitest": "^3.1.4",
    "webpack": "^5.99.9"
  },
  "dependencies": {
    "@carbon/icons-react": "^11.65.0",
    "@carbon/react": "^1.83.0",
    "@langchain/core": "^0.3.75",
    "@langchain/ollama": "^0.2.4",
    "fuzzy": "^0.1.3"
  }
}
